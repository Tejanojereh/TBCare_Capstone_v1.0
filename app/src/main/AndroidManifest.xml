<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tbcare_capstone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/n_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ForgotPassword_ChangePassActivity"></activity>
        <activity android:name=".Answer_Security_QuestionsActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".RegisterPatientStart" />
        <activity android:name=".My_Patients" />
        <activity android:name=".My_Partner_Info" />
        <activity android:name=".ChoosePartner" />
        <activity android:name=".Choose_Account_Type" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".My_Schedule_Patient" />
        <activity android:name=".Menu_Patient" />
        <activity android:name=".Note_Patient" />
        <activity android:name=".Menu_TBPartner" />
        <activity android:name=".View_and_Update_Medication_Progress" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".Account_TBPartner" />
        <activity android:name=".TreatmentDetails" />
        <activity android:name=".Partner_Info" />
        <activity android:name=".Myaccount_partner" />
        <activity android:name=".RegisterPartner" />
        <activity android:name=".test" />
        <activity android:name=".RegisterPatient" />
        <activity
            android:name=".EditNote"
            android:label="@string/title_activity_edit_note"
            android:parentActivityName=".Notes"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.android.tbcare_capstone.Notes" />
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>